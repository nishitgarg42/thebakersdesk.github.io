<div class="admin-h1">
  <h1>The Baker's Desk Admin Panel for Upload</h1>
</div>


<div class="container">
  <h1>Upload Files</h1>
  <%= typeof msg != "undefined" ? msg : "" %>
  <h1><%= typeof afterUpload != "undefined" ? afterUpload : "" %></h1>

  <form action="/adminupload" method="POST" enctype="multipart/form-data">
    <div class="file-field input-field">
      <div class="btn">
        <span>File</span>
        <input name="myImage" type="file" multiple>
      </div>
      <div class="file-path-wrapper">
        <input class="file-path validate" type="text" placeholder="Upload one or more files">
      </div>
    </div>
    <button type="submit" class="btn" name="button">Submit</button>
  </form>
</div>

<div class="m-5 p-3">
  <a class="border rounded-pill p-3 " href="/adminview">View Photos</a>
</div>



